{
  "expo": {
    "name": "Recashly",
    "slug": "Recashly",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "enabled": true,
      "checkAutomatically": "ON_LOAD",
      "fallbackToCacheTimeout": 0,
      "url": "https://u.expo.dev/83ac70ce-b532-4c7d-938d-e8b49b2995e5"
    },
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#1a3a52"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.recashly.app"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#1a3a52"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.recashly.app",
      "permissions": [
        "android.permission.CAMERA",
        "android.permission.RECORD_AUDIO"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-build-properties",
        {
          "android": {
            "kotlinVersion": "1.9.25"
          }
        }
      ],
      [
        "expo-camera",
        {
          "cameraPermission": "Izinkan $(PRODUCT_NAME) mengakses kamera untuk mengambil foto struk."
        }
      ],
      [
        "expo-image-picker",
        {
          "photosPermission": "Izinkan $(PRODUCT_NAME) mengakses galeri foto untuk memilih gambar struk."
        }
      ]
    ],
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "83ac70ce-b532-4c7d-938d-e8b49b2995e5"
      }
    }
  }
}